{
  "name": "aws-certification-platform",
  "version": "1.0.0",
  "private": true,
  "description": "AWS Certification Practice Platform - Serverless exam simulation environment",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "setup": "node scripts/setup-dev.js",
    "deploy": "amplify publish",
    "deploy:dev": "amplify publish --environment dev",
    "deploy:staging": "amplify publish --environment staging",
    "deploy:prod": "amplify publish --environment prod",
    "amplify:init": "amplify init",
    "amplify:push": "amplify push",
    "amplify:status": "amplify status",
    "amplify:console": "amplify console",
    "postinstall": "npm run setup:check"
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.0",
    "@cloudscape-design/design-tokens": "^3.0.0",
    "@cloudscape-design/global-styles": "^1.0.0",
    "aws-amplify": "^6.0.0",
    "@aws-amplify/ui-react": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "@tanstack/react-query": "^5.0.0",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0",
    "pdfkit": "^0.13.0",
    "csv-parser": "^3.0.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.5.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}