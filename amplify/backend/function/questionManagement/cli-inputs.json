{
  "version": "1",
  "resourceName": "questionManagement",
  "functionName": "questionManagement",
  "roleName": "questionManagementLambdaRole",
  "lambdaLayers": [],
  "runtime": "nodejs18.x",
  "timeout": 25,
  "memorySize": 512,
  "build": true,
  "dependsOn": [
    {
      "category": "api",
      "resourceName": "awscertificationplatform",
      "attributes": [
        "GraphQLAPIIdOutput",
        "GraphQLAPIEndpointOutput"
      ]
    },
    {
      "category": "storage",
      "resourceName": "awscertplatformdb",
      "attributes": [
        "Name",
        "Arn"
      ]
    }
  ],
  "environmentVariables": {
    "QUESTIONS_TABLE_NAME": {
      "Ref": "storageawscertplatformdbName"
    },
    "IMPORT_BUCKET_NAME": {
      "Ref": "storageawscertplatformstorageBucketName"
    }
  }
}